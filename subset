class Solution:
    def subsets(self, nums: list[int]) -> list[list[int]]:
        n = len(nums)
        total_subsets = (1<<n)
        ans = []
        for num in range(total_subsets):
            temp = []
            for i in range(n):
                if((num&(1<<i))!=0):
                    temp.append(nums[i])
            ans.append(temp)
        return ans
ob = Solution()
nums = [1, 2, 3]
print(ob.subsets(nums)
